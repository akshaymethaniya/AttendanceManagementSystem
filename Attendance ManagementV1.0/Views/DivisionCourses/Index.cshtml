@model IEnumerable<Attendance_ManagementV1._0.Models.DivisionCourse>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.courses.CourseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.divisions.DivisionName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.courses.CourseName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.divisions.DivisionName)
        </td>
      
    </tr>
}

</table>
